# Supabase Configuration - Staging Cloud
NEXT_PUBLIC_SUPABASE_URL=https://your-staging-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_staging_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_staging_service_role_key

# Environment
NODE_ENV=development
NEXT_PUBLIC_APP_URL=https://admin-staging.your-domain.com

# Database (direct connection for admin operations)
DATABASE_URL=postgresql://postgres.your-staging-project-ref:your-password@aws-0-region.pooler.supabase.com:6543/postgres

# Staging-specific configurations
NEXT_PUBLIC_ENVIRONMENT=staging