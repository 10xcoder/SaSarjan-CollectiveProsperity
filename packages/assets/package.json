{
  "name": "@repo/assets",
  "version": "0.1.0",
  "description": "Centralized assets package for SaSarjan App Store",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./logos/*": "./logos/*",
    "./icons/*": "./icons/*",
    "./favicons/*": "./favicons/*"
  },
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "src",
    "logos",
    "icons",
    "favicons"
  ],
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "license": "MIT"
}